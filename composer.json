{
    "name": "etten/migrations",
    "description": "Database migrations runner",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "nette/tester": "^1.7",
        "mockery/mockery": "^0.9",
        "squizlabs/php_codesniffer": "~2.6.0",
        "etten/codestyle": "^2.0",
        "dibi/dibi": "^2.3",
        "doctrine/dbal": "^2.5",
        "nette/database": "^2.3",
        "nette/di": "^2.3",
        "nette/utils": "^2.3",
        "nextras/dbal": "^2.0",
        "symfony/console": "^2.8"
    },
    "suggest": {
        "dibi/dibi": "to use DibiAdapter",
        "nette/database": "to use NetteAdapter",
        "nextras/dbal": "to use NextrasAdapter",
        "doctrine/dbal": "to use DoctrineAdapter",
        "symfony/console": "to use Symfony commands"
    },
    "autoload": {
        "psr-4": {
            "Etten\\Migrations\\": "src/"
        }
    }
}
